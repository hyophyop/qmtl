# Seamless stack defaults
QMTL_SEAMLESS_COORDINATOR_URL=http://seamless-coordinator:8080
QMTL_SEAMLESS_ARTIFACTS=1
QMTL_SEAMLESS_ARTIFACT_DIR=/var/lib/qmtl/seamless-artifacts
QMTL_SEAMLESS_FP_MODE=canonical
QMTL_SEAMLESS_PUBLISH_FP=true
QMTL_SEAMLESS_PREVIEW_FP=false
QMTL_SEAMLESS_EARLY_FP=false
QMTL_SEAMLESS_SLA_PRESET=baseline
QMTL_SEAMLESS_CONFORMANCE_PRESET=strict-blocking

# Distributed coordinator runtime
SEAMLESS_COORDINATOR_ID=local-dev
SEAMLESS_COORDINATOR_BIND=0.0.0.0:8080
SEAMLESS_COORDINATOR_PROMETHEUS_PORT=9102
SEAMLESS_COORDINATOR_REDIS_DSN=redis://redis:6379/3
SEAMLESS_COORDINATOR_QUESTDB_DSN=postgresql://qmtl:qmtl@questdb:8812/qdb

# QuestDB credentials used by the Seamless provider
QUESTDB_POSTGRES_USER=qmtl
QUESTDB_POSTGRES_PASSWORD=qmtl
QUESTDB_POSTGRES_DB=qdb

# Object storage bucket for Seamless artifacts
MINIO_ROOT_USER=seamless
MINIO_ROOT_PASSWORD=seamless123
MINIO_BUCKET=seamless-artifacts
MINIO_CONSOLE_PORT=9001

# Optional OpenTelemetry/metrics endpoints
OTEL_EXPORTER_OTLP_ENDPOINT=
