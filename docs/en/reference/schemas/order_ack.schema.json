{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://qmtl.dev/schemas/order_ack.schema.json",
  "title": "OrderAck",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "order_id": { "type": "string" },
    "client_order_id": { "type": "string" },
    "status": {
      "type": "string",
      "enum": [
        "accepted",
        "rejected",
        "canceled",
        "filled",
        "partially_filled",
        "expired"
      ]
    },
    "reason": { "type": ["string", "null"] },
    "broker": { "type": "string" },
    "raw": { "type": "object" }
  },
  "required": ["order_id", "status", "broker"]
}
