{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://qmtl.dev/schemas/execution_fill_event.schema.json",
  "title": "ExecutionFillEvent",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "order_id": { "type": "string" },
    "client_order_id": { "type": "string" },
    "correlation_id": { "type": "string" },
    "symbol": { "type": "string" },
    "side": { "type": "string", "enum": ["BUY", "SELL"] },
    "quantity": { "type": "number" },
    "price": { "type": "number" },
    "commission": { "type": "number" },
    "slippage": { "type": "number" },
    "market_impact": { "type": "number" },
    "tif": { "type": "string" },
    "fill_time": { "type": "integer" },
    "status": { "type": "string" },
    "seq": { "type": "integer" },
    "etag": { "type": "string" }
  },
  "required": [
    "order_id",
    "symbol",
    "side",
    "quantity",
    "price",
    "commission",
    "slippage",
    "market_impact",
    "tif",
    "fill_time",
    "status",
    "seq",
    "etag"
  ]
}
