{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://qmtl.dev/schemas/event_queue_updated.schema.json",
  "title": "QueueUpdated",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "type": { "const": "QueueUpdated" },
    "version": { "type": "integer", "minimum": 1 },
    "tags": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "interval": { "type": "integer", "minimum": 1 },
    "queues": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 0
    },
    "etag": { "type": "string" },
    "ts": { "type": "string", "format": "date-time" }
  },
  "required": ["type", "version", "tags", "interval", "queues", "etag", "ts"]
}

